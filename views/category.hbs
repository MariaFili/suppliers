
<h2>Категория {{name}}</h2>

  <table class="title">
    
    <tr class="title">

      <th class="maintable2 title">Объявление</th>
      <th class="maintable2 title">Компания</th>
      <th class="maintable2 title">Округ</th>
      <th class="maintable2 title">Город</th>
      <th class="maintable2 title">Дата</th>
    </tr>


  </table>
  <div>{{#each allCategories}}
  <table class="maintable2">
    
    <tr class="maintable2">

      <th class="maintable2"> 
        <a href="/ad/{{this._id}}">
        {{this.title}}
      </a></th>
      <th class="maintable2">{{this.author.info.companyname}}</th>
      <th class="maintable2">{{this.district}}</th>
      <th class="maintable2">{{this.city}}</th>
      <th class="maintable2">{{this.expdate}}</th>
    </tr>


  </table>

  {{/each}}
