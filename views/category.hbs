
<h2>Категория {{name}}</h2>



  {{#each allCategories}}
  <table class="table table-striped table-bordered" style="width:100%">
    <thead class="thead-dark">
    <tr>

      <th> <a href="/ad/{{this.id}}">
        {{this.title}}
      </a>
        </th>
      <th>{{this.author.info.companyname}}</th>
      <th>{{this.district}}</th>
      <th>{{this.city}}</th>
      <th>{{this.expdate}}</th>
      
    </tr>
</thead>

  </table>
  
  {{/each}}
  